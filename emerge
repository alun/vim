
fu! <SID>NextConflict()
  let l:status = system("git status") 
  exe "e " . matchstr(l:status, "\\%(both modified:\\s*\\zs\\%(\\w\\|/\\|\\.\\)\\+\\ze\n\\)\\|\\%(both added:\\s*\\zs\\%(\\w\\|/\\|\\.\\)\\+\\ze\n\\)")
endfu

nm ,cc :!git add %<CR><CR>
nm ,cf /<<<<<<<CR>
nm ,cs :!git status<CR>
nm ,ut V/=======<CR>d/>>>>>>><CR>dd:w<CR>
nm ,um dd/=======<CR>V/>>>>>>><CR>d:w<CR>
nm ,cn :call <SID>NextConflict()<CR>
